{
  "name": "movie-api",
  "version": "1.0.0",
  "description": "## Instructions In the initial commit to this project, you have been provided with a CSV file that contains data on the American Film Institute's (AFI) Top 100 Movies. Your task is to create a database backed Express API for access the details of these films.",
  "main": "index.js",
  "scripts": {
    "migrate:dev": "./node_modules/.bin/env-cmd ./node_modules/.bin/sequelize db:migrate --env development --config ./config/sequelize.js",
    "migrate:prod": "./node_modules/.bin/sequelize db:migrate --env production --config ./config/sequelize.js",
    "server:dev": "./node_modules/.bin/nodemon index.js",
    "server:prod": "node index.js",
    "dev": "./node_modules/.bin/env-cmd ./node_modules/.bin/nodemon index.js",
    "start": "npm run migrate:prod && npm server:prod",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ainsley929/movie-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ainsley929/movie-api/issues"
  },
  "homepage": "https://github.com/Ainsley929/movie-api#readme",
  "devDependencies": {
    "body-parser": "^1.19.0",
    "eslint": "^6.7.2",
    "express": "^4.17.1",
    "mysql2": "^2.0.2",
    "nodemon": "^2.0.1",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1"
  },
  "dependencies": {
    "env-cmd": "^10.0.1"
  }
}
